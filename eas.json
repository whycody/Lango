{
  "cli": {
    "version": ">= 15.0.6",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "channel": "preview",
      "environment": "preview"
    },
    "test": {
      "android": {
        "buildType": "apk"
      },
      "channel": "test",
      "environment": "development"
    },
    "production": {
      "channel": "production",
      "android": {
        "buildType": "app-bundle"
      },
      "environment": "production",
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}
